(import (core list) (chezscheme)
        (core for))
(display (build-list 3 (lambda (x) x)))
(display (check-duplicates '(1 2 3)))
(display (check-duplicates '(1 2 3 1)))
(newline)
(display (remove-duplicates '(1 a 2 3 2 a b)))
(newline)
(for i <- (in-list '(1 2 3 4)) (display i))
(for j <- (in-vector '#(1 2 3 4)) (display j))
(newline)
(for (i j) <- (in-alist '((1 . 2) (3 . 4) (a . b)))
  (display "key is ") (display i)
  (display ",value is ") (display j)
  (newline))
(display (for/sum i <- (in-list '(1 2 3 4)) (- i)))
(newline)
(display (for/list i <- (in-list '(1 2 3 4)) (- i)))
(newline)
(display (for/list (i j) <- (in-alist '((1 . 2) (3 . 4) (a . b)))
           (cons j i)))
(newline)
(display (range 5))
(newline)
(display (for/and i <- (in-list '(1 2 3 4)) (> i 0)))
(display (for/and i <- (in-list '(1 2 3 -4)) (> i 0)))
(display (for/or i <- (in-list '(1 2 3 4)) (< i 0)))
(display (for/or i <- (in-list '(1 2 3 -4)) (< i 0)))
(newline)
(for i <- (in-string "testabc") (display i))
